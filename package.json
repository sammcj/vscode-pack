{
  "name": "sammcj-vscode-pack",
  "displayName": "sammcj-vscode-pack",
  "description": "Sam's vscode extension pack",
  "version": "0.0.4",
  "publisher": "sammcleod",
  "icon": "images/derp.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/sammcj/vscode-pack"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "scripts": {
    "next-version": "git describe --tags --abbrev=0 | awk -F. '{OFS=\".\"; $NF+=1; print $0}'",
    "bump-tag": "git tag $(npm run next-version --silent)",
    "bump-version": "npm version from-git",
    "bump-and-push": "npm run bump-tag && npm run bump-version && git push --tags"
  },
  "categories": [
    "Extension Packs"
  ],
